//count no of digits a number has 
input  = 1382
output = 4


import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
      int num = 1382;
      int count = 0;
      while (num!=0)
      {
          num = num/10;
          count ++;
      }
      System.out.println(count);
      
    }
}
