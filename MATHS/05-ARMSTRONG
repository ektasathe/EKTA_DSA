
// find a no is armstrong or not(3 digit no = cube of each digit)
class Solution {
    static String armstrongNumber(int n) {
        // code here
        int s=n;
        int ans=0;
        while(n!=0){
            int r=n%10;
            ans=ans+(r*r*r);
            n=n/10;
        }
        if(s==ans)
            return "true";
        return "false";
    }
}

